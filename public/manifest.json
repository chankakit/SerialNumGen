{
  "manifest_version": 3,
  "name": "Serial Num Generator",
  "description": "Serial Num Generator",
  "version": "1.0",
  "action": {
    "default_icon": {
      "16": "icons/16.png",
      "32": "icons/32.png",
      "48": "icons/48.png"
    }
  },
  "background": {
    "service_worker": "background.js"
  },
  "permissions": [
    "tabs"
  ]
}